{
    "name": "Gorse Export Plugin",
    "url": "",
    "description": "Export PostHog events into Gorse",
    "main": "index.js",
    "posthogVersion": ">= 1.25.0",
    "config": [
        {
            "key": "eventPath",
            "name": "Path of the url where events will go to. In the format of http://51.89.15.39:8087/api/<feedback/item/user>",
            "type": "string",
            "required": true
        },
        {
            "key": "eventMethodType",
            "name": "The type of method for the event url",
            "default": "POST",
            "type": "string"
        },
        {
            "key": "eventsToInclude",
            "name": "Events to include",
            "type": "string",
            "hint": "Comma separated list of events to include"
        }
    ]
}
